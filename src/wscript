def build(bld):
  bld(source = """
  emitters/szlunique.cc
  emitters/szltopheap.cc
  emitters/szlsketch.cc
  emitters/szltop.cc
  emitters/szlquantile.cc
  emitters/szlcomputequantiles.cc
  public/hashutils.cc
  """,
      features ='cxx cxxshlib',
      name     = 'szaru',
      target   = 'szaru',
      lib = 'ssl',
      includes     = '.')
